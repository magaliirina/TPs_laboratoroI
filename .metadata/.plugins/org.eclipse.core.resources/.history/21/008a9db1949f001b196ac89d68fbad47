#include <stdio.h>
#include <stdlib.h>
#include <stdio_ext.h>
#include "utn.h"
#define CANTNUMEROS 2

int main(void) {

float resultado;
for(int i=0;i<CANTNUMEROS;i++){
utn_getnumero(&resultado, "Ingrese un numero");
}

}

