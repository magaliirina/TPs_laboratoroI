#include <stdio.h>
#include <stdlib.h>
#include <stdio_ext.h>
#include "utn.h"
#define CANTNUMEROS 2

int main(void) {
float resultado;
int opcion;

utn_getnumero(&resultado, "\nIngrese un numero", "\nOcurrio un Error",2, CANTNUMEROS);
utn_getnumeroRango(&opcion, "Ingrese una opcion", "ERROR", 2, 3, 1);
printf("%d",opcion);





return 0;
}

