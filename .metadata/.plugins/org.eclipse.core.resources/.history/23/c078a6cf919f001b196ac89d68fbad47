#include <stdio.h>
#include <stdlib.h>
#include <stdio_ext.h>


void utn_imprimirMensaje (char* mensaje){
	printf("%s",mensaje);
}
int utn_getnumero(int* numero, char* mensaje){
	int retorno=-1;

	if(numero!=NULL&&mensaje!=NULL){

		printf("%s", mensaje);
		scanf("%d",&numero);
		retorno=0;
	}

	return retorno;
}


