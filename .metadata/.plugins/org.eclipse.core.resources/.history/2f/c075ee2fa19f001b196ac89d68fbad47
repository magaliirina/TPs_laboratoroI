#include <stdio.h>
#include <stdlib.h>
#include <stdio_ext.h>
#include "utn.h"

int main(void) {
float numeroA;
float numeroB;
int opcion;

utn_getnumero(&numeroA, "Ingrese un numero\n", "Error\n", 2);
utn_getnumero(&numeroB, "Ingrese un numero\n", "Error\n", 2);
if(utn_getnumeroRango(&opcion, "Ingrese una opcion\n", "ERROR\n", 2, 5, 1)){
	switch(opcion){
	case 1:
	case 2:
	case 3:
	case 4:
	case 5:
	}
}






return 0;
}

